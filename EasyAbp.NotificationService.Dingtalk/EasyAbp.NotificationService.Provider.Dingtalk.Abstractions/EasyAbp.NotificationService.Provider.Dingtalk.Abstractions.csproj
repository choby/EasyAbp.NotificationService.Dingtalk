    <ItemGroup>
      <PackageReference Include="AlibabaCloud.SDK.Dingtalk" Version="2.0.28" />
      <PackageReference Include="EasyAbp.NotificationService.Domain.Shared" Version="2.3.0" />
    </ItemGroup>


</Project>
